int ackermann(int m, int n){
    int resultado;
    if(m==0){
        resultado=n+1;
        }
        else
        if(n==0){
            resultado=ackermann(m-1,1)
            }
            else{
                resultado=ackermann(m-1,ackermann(m,n-1));
                }
                return resultado;
    }
